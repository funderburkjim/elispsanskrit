../grammar/lisp/sandhi.el:      (setq w1 (guna v2)) ; a tokarr
../grammar/lisp/sandhi.el:(defun gunate-final-vowel (tok &optional vrddhiP parts)
../grammar/lisp/sandhi.el: (let (ans gunaP)
../grammar/lisp/sandhi.el:   ; when vrddhiP is nil (or absent) gunaP = t (so we gunate vowel)
../grammar/lisp/sandhi.el:   ; when vrddhiP is 't', gunaP = nil (so we vrddhate vowel)
../grammar/lisp/sandhi.el:  (setq gunaP (not vrddhiP))
../grammar/lisp/sandhi.el:      (setq v (if gunaP (guna v0) (vrddhi v0)))
../grammar/lisp/sandhi.el:      ; only gunate when v0 is a short simple vowel AND
../grammar/lisp/sandhi.el:       (setq v (if gunaP (guna v0) (vrddhi v0)))
